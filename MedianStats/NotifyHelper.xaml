<UserControl x:Class="MedianStats.NotifyHelper"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:MedianStats"
		mc:Ignorable="d"
		Height="429.508" Width="544.262">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<StackPanel Grid.Row="0">
			<Label Content="{DynamicResource Show_Matches_Description}">
			</Label>
			<TextBox x:Name="testString"/>
			<Button Click="Button_Click" Content="{DynamicResource Show_Matches}"/>
		</StackPanel>
		<ListBox x:Name="matchesList" Grid.Row="1"/>
	</Grid>
</UserControl>
